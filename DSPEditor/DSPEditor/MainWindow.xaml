<Window x:Class="DSPEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DSPEditor"
        mc:Ignorable="d"
        Title="DSPEditor" Height="450" Width="800" HorizontalAlignment="Center" VerticalAlignment="Center" ResizeMode="CanMinimize" Icon="DSPEditor.png" FontFamily="Rockwell" BorderBrush="Black" FontSize="12">
    <Grid Background="#FF3F3F46" Opacity="0.95">
        <DockPanel>
            <Menu DockPanel.Dock="Top" Background="#FFF0F0F0" FontSize="12">
                <MenuItem Header="_File">
                    <MenuItem Header="_Open"/>
                    <MenuItem Header="_Close"/>
                    <MenuItem Header="_Save"/>
                    <MenuItem Header="_Load"/>
                    <MenuItem Header="_Export">
                        <MenuItem Header="_MP3 File"/>
                        <MenuItem Header="_WAV File"/>
                    </MenuItem>
                </MenuItem>
                <MenuItem Header="_Effect">
                    <MenuItem Header="_Modulation effects">
                        <MenuItem Header="_Flanger"/>
                        <MenuItem Header="_Ring modulation"/>
                        <MenuItem Header="_Tremolo"/>
                        <MenuItem Header="_Flanger"/>
                        <MenuItem Header="_Chorus"/>
                    </MenuItem>
                    <MenuItem Header="_Dynamic effects">
                        <MenuItem Header="_Fuzz"/>
                    </MenuItem>
                    <MenuItem Header="_Time-based effects">
                        <MenuItem Header="_Echo"/>
                            <MenuItem Header="_Reverb"/>
                                <MenuItem Header="_Delay"/>
                    </MenuItem>
                    <MenuItem Header="_Customm">
                        <MenuItem Header="_Wah-wah"/>
                        <MenuItem Header="_Vibrato"/>
                    </MenuItem>
                </MenuItem>
                <MenuItem Header="_Generate">
                    <MenuItem Header="_Noise"/>
                </MenuItem>
            </Menu>
            <StackPanel></StackPanel>
        </DockPanel>
    </Grid>
</Window>
